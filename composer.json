{
    "name": "soyuka/esql",
    "description": "PHP Extended Structured Query Language",
    "type": "library",
    "require": {
        "phpmyadmin/sql-parser": "^5.4",
        "jms/parser-lib": "^1.0",
        "symfony/dependency-injection": "^5.2"
    },
    "require-dev": {
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/config": "^5.2",
        "friendsofphp/php-cs-fixer": "^3.3",
        "vimeo/psalm": "^4.3",
        "symfony/phpunit-bridge": "^5.2",
        "hautelook/alice-bundle": "^2.8",
        "symfony/http-client": "^5.2",
        "symfony/browser-kit": "^5.2",
        "phpspec/prophecy-phpunit": "^2.0",
        "api-platform/core": "dev-main",
        "doctrine/orm": "^2.8",
        "jane-php/automapper": "dev-next",
        "symfony/serializer": "^5.2",
        "symfony/uid": "^5.2",
        "jane-php/automapper-bundle": "dev-next",
        "rector/rector": "^0.12.21",
        "symfony/profiler-pack": "^1.0"
    },
    "suggest": {
        "doctrine/orm": "To use doctrine metadata.",
        "api-platform/core": "To use the API Platform bridge.",
        "phpdocumentor/reflection-docblock": "To support extracting metadata from PHPDoc.",
        "jane-php/automapper": "To use JanePHP for automapping.",
        "symfony/serializer": "To use Symfony for automapping."
    },
    "license": "MIT",
    "authors": [
        {
            "name": "soyuka",
            "email": "soyuka@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Soyuka\\ESQL\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soyuka\\ESQL\\Tests\\": "tests/"
        }
    }
}
